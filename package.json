{
  "name": "coronavirus-visualization",
  "version": "1.0.0",
  "description": "COVID-19 visualization",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.ts --progress --colors --mode=production",
    "start": "webpack-dev-server --config webpack.dev.ts --progress --colors --open --mode=development",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prettier": "prettier --write \"./**/*.{json,ts,tsx,scss}\""
  },
  "keywords": [
    "react",
    "coronavirus",
    "covid-19",
    "visualization"
  ],
  "author": "Nathaniel C. Hall",
  "license": "MIT",
  "devDependencies": {
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/jest": "^24.9.1",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-test-renderer": "^16.9.4",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-unicorn": "^12.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "style-loader": "^1.3.0",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "@nathanielhall/useapi": "^1.0.3",
    "@types/enzyme": "^3.10.8",
    "@types/recharts": "^1.8.19",
    "axios": "^0.21.1",
    "date-fns": "^2.16.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "recharts": "^1.8.5"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "cobertura"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src$1",
      "^components(.*)$": "<rootDir>/src/components$1"
    },
    "modulePaths": [
      "src"
    ],
    "setupFilesAfterEnv": [
      "./enzyme.config.ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.tsx?$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "verbose": true
  }
}

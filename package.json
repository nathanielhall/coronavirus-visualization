{
  "name": "coronavirus-visualization",
  "version": "1.0.0",
  "description": "COVID-19 visualization",
  "main": "index.js",
  "scripts": {
    "build": "webpack --progress --colors --mode=production",
    "start": "webpack-dev-server --config webpack.config.ts --progress --colors --open --mode=development",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prettier": "prettier --write \"./**/*.{json,ts,tsx,scss}\""
  },
  "keywords": [
    "react",
    "coronavirus",
    "covid-19",
    "visualization"
  ],
  "author": "Nathaniel C. Hall",
  "license": "MIT",
  "devDependencies": {
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.9.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-test-renderer": "^16.9.1",
    "@types/webpack": "^4.39.8",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "eslint-plugin-unicorn": "^12.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "style-loader": "^1.0.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "@material-ui/pickers": "^3.2.10",
    "@types/enzyme": "^3.10.5",
    "@types/recharts": "^1.8.9",
    "axios": "^0.21.1",
    "date-fns": "^2.11.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "recharts": "^1.8.5"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "cobertura"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src$1",
      "^components(.*)$": "<rootDir>/src/components$1"
    },
    "modulePaths": [
      "src"
    ],
    "setupFilesAfterEnv": [
      "./enzyme.config.ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.tsx?$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "verbose": true
  }
}
